		</tr>
		
		<tr>
			<td>Username :</td>
			<td><input type="text" name="user" placeholder="Masukkan Username" />
		</tr>

		<tr>
			<td>Password :</td>
			<td><input type="text" name="pass" placeholder="Masukkan Password" />
		</tr>

		<tr>
			<td>Email :</td>
			<td><input type="text" name="mail" placeholder="Masukkan Alamat email" />
		</tr>

		
		<tr>
