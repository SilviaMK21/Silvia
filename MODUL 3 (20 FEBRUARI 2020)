	<table border=0>
	
		<tr>
			<td>Nama Lengkap :</td>
			<td><input type="text" name="nama" placeholder="Masukkan Nama Lengkap"  required /> 
		</tr>
		
		<tr>
			<td>Username :</td>
			<td><input type="text"  placeholder="Masukkan Username" required />
		</tr>

		<tr>
			<td>Password :</td>
			<td><input type="password" placeholder="Masukkan Password" required />
		</tr>

		<tr>
			<td>Email :</td>
			<td><input type="text" placeholder="Masukkan Alamat email" required />
		</tr>

		
		<tr>
			<td>Telepon :</td>
			<td><input type="text" name="tel" maxlength="12" required />
		</tr>


		<tr>
			<td>
			</td>
			<td>
				<input type="submit" value="Buat Akun Saya" />
			</td>
		</tr>

	</table>
	
